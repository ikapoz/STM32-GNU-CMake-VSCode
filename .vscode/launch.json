{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) attach",
            "type": "gdb",
            "request": "attach",
            "target": ":61234",
            "remote": true,
            "executable": "${workspaceFolder}\\build\\main",
            "gdbpath": "${workspaceFolder}\\..\\stm32-utils\\gcc\\bin\\arm-none-eabi-gdb.exe",
            "cwd": "${workspaceRoot}",
            "autorun": [
                "load"
            ],
            "preLaunchTask": "Build Debug and start GDB server"
        }
    ]
}